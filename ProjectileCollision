using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class ProjectileCollision : MonoBehaviour {

	public int damage;


	void Awake()
		{
			var enemyHealth = GameObject.FindWithTag("Enemy").GetComponent<EnemyHealth>().currentHealth;
		}
	private void OnTriggerEnter2D(Collider2D other)
		{	
			enemyHealth -= damage;
			//Destroy(gameObject);
		} 
}
